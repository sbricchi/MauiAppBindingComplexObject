<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:classes="clr-namespace:MauiAppBindingComplexObject"
             x:Class="MauiAppBindingComplexObject.MyView"
             x:DataType="classes:MyItem">
    <VerticalStackLayout HeightRequest="200">
        <!--<Label Text="{Binding IDText}" />
        <Label Text="{Binding NameText}" />
        <Label Text="{Binding DescriptionText}" />-->
        <Label Text="{Binding ID}" />
        <Label Text="{Binding Name}" />
        <Label Text="{Binding Description}" />
    </VerticalStackLayout>
</ContentView>
